{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest.js/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest.js/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest.js",
  "projects": {
    "nest.js": {
      "type": "application",
      "root": "apps/nest.js",
      "entryFile": "main",
      "sourceRoot": "apps/nest.js/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest.js/tsconfig.app.json"
      }
    },
    "basic_crud_service": {
      "type": "application",
      "root": "apps/basic_crud_service",
      "entryFile": "main",
      "sourceRoot": "apps/basic_crud_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/basic_crud_service/tsconfig.app.json"
      }
    }
  }
}